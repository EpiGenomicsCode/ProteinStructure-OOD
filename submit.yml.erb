<%-
  rc_queue = "open".to_s
  case session_type     
  when "ESMFold"
      slurm_args = ["--nodes", "1", "--ntasks-per-node", "1", "--cpus-per-task", "8", "--gpus", "1", "--partition", rc_queue]
  else
      slurm_args = ["--nodes", "1", "--ntasks-per-node", "1", "--cpus-per-task", "8", "--partition", "open"]
  end    
-%>
---
batch_connect:
  template: "basic"
script:
  native:
    <%- slurm_args.each do |arg| -%>
      - "<%= arg %>"
    <%- end -%>